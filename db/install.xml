<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/logiccircuit/db" VERSION="2013070" COMMENT="XMLDB file for Moodle question logic"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="question_logiccircuit" COMMENT="Properties of the logic circuit questions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="questionid"/>
        <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Foreign key references question.id" PREVIOUS="id"/>
        <FIELD NAME="initialstate" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="questionid" NEXT="endstate" COMMENT="The initial state of the logic circuit editor"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="question"/>
        <KEY NAME="question" TYPE="foreign" FIELDS="questionid" REFTABLE="question" REFFIELDS="id" PREVIOUS="primary" />
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
